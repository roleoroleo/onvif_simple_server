<?xml version="1.0" encoding="UTF-8"?>
<SOAP-ENV:Envelope xmlns:SOAP-ENV="http://www.w3.org/2003/05/soap-envelope"
                   xmlns:tds="http://www.onvif.org/ver10/device/wsdl"
                   xmlns:tt="http://www.onvif.org/ver10/schema">
    <SOAP-ENV:Body>
        <tds:GetNetworkInterfacesResponse>
            <tds:NetworkInterfaces token="NetworkInterfaceToken">
                <tt:Enabled>true</tt:Enabled>
                <tt:Info>
                    <tt:Name>%INTERFACE%</tt:Name>
                    <tt:HwAddress>%MAC_ADDRESS%</tt:HwAddress>
                    <tt:MTU>%MTU%</tt:MTU>
                </tt:Info>
                <tt:IPv4>
                    <tt:Enabled>true</tt:Enabled>
                    <tt:Config>
                        <tt:Manual>
                            <tt:Address>%IP_ADDRESS%</tt:Address>
                            <tt:PrefixLength>%NETMASK%</tt:PrefixLength>
                        </tt:Manual>
                        <tt:DHCP>false</tt:DHCP>
                    </tt:Config>
                </tt:IPv4>
                <tt:IPv6>
                    <tt:Enabled>false</tt:Enabled>
                </tt:IPv6>
            </tds:NetworkInterfaces>
        </tds:GetNetworkInterfacesResponse>
    </SOAP-ENV:Body>
</SOAP-ENV:Envelope>
