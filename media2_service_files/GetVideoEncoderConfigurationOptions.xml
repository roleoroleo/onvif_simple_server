<?xml version="1.0" encoding="UTF-8"?>
<SOAP-ENV:Envelope xmlns:SOAP-ENV="http://www.w3.org/2003/05/soap-envelope"
                   xmlns:tr2="http://www.onvif.org/ver20/media/wsdl"
                   xmlns:tt="http://www.onvif.org/ver10/schema">
    <SOAP-ENV:Body>
        <tr2:GetVideoEncoderConfigurationOptionsResponse>
            <tr2:Options GovLengthRange="0 100"
                         FrameRatesSupported="60 55 50 45 40 35 30 25 20 15 10 5"
                         ProfilesSupported="%H264PROFILE%"
                         ConstantBitRateSupported="false"
                         GuaranteedFrameRateSupported="false">
                <tt:Encoding>%VIDEO_ENCODING%</tt:Encoding>
                <tt:QualityRange>
                    <tt:Min>0</tt:Min>
                    <tt:Max>100</tt:Max>
                </tt:QualityRange>
                <tt:ResolutionsAvailable>
                    <tt:Width>%WIDTH%</tt:Width>
                    <tt:Height>%HEIGHT%</tt:Height>
                </tt:ResolutionsAvailable>
                <tt:BitrateRange>
                    <tt:Min>10</tt:Min>
                    <tt:Max>5000</tt:Max>
                </tt:BitrateRange>
            </tr2:Options>
        </tr2:GetVideoEncoderConfigurationOptionsResponse>
    </SOAP-ENV:Body>
</SOAP-ENV:Envelope>
